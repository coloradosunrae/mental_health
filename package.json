{
  "name": "mental-health",
  "version": "1.0.0",
  "description": "Mern Demo",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.4",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "install": "^0.12.2",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^5.5.6",
    "morgan": "^1.9.1",
    "node-sass": "^4.12.0",
    "npm": "^6.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-input-mask": "^2.0.4",
    "react-materialize": "^3.2.2",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-select": "^2.4.3",
